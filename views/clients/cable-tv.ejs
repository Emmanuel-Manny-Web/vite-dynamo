      <img src="images/vite-logo.jpeg" alt="icon" class="loading-icon" style="border: none; border-radius: 50%;">
    </div>
    <!-- * loader -->

    <!-- App Header -->
    <div class="appHeader bg-white text-primary">
      <div class="left">
        <a href="#" class="headerButton" data-bs-toggle="modal" data-bs-target="#sidebarPanel">
          <ion-icon name="menu-outline" role="img" class="md hydrated"></ion-icon>
        </a>
      </div>
      <div class="pageTitle">
        <img src="images/vite-logo.jpeg" alt="logo" class="logo" style="border: none; border-radius: 50%;">
      </div>
      <div class="right">

      </div>
    </div>
    <!-- * App Header -->


    <!-- App Capsule -->
    <div id="appCapsule">

      <!-- Wallet Card -->
      <%- include('../partials/clients/header') %>
        <!-- Wallet Card -->

        <!-- Deposit Action Sheet -->

        <style type="text/css">
          .copy-text {
            cursor: pointer;
          }
        </style>
        <%- include('../partials/clients/fund-modal') %>
        <!-- * Deposit Action Sheet -->

        <!-- Send Action Sheet -->
        <div class="modal fade action-sheet" id="sendActionSheet" tabindex="-1" role="dialog">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title">Send Money</h5>
              </div>
              <div class="modal-body">
                <div class="action-sheet-content">
                  <form>
                    <div class="form-group basic">
                      <div class="input-wrapper">
                        <label class="label" for="account2">From</label>
                        <select class="form-control custom-select" id="account2">
                          <option value="0">Savings (*** 5019)</option>
                          <option value="1">Investment (*** 6212)</option>
                          <option value="2">Mortgage (*** 5021)</option>
                        </select>
                      </div>
                    </div>

                    <div class="form-group basic">
                      <div class="input-wrapper">
                        <label class="label" for="text11">To</label>
                        <input type="email" class="form-control" id="text11" placeholder="Enter bank ID">
                        <i class="clear-input"><ion-icon name="close-circle" role="img"
                            class="md hydrated"></ion-icon></i>
                      </div>
                    </div>

                    <div class="form-group basic">
                      <label class="label">Enter Amount</label>
                      <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <span class="input-group-text" id="input14">$</span>
                        </div>
                        <input type="text" class="form-control form-control-lg" placeholder="0">
                      </div>
                    </div>



                    <div class="form-group basic">
                      <button type="button" class="btn btn-primary btn-block btn-lg"
                        data-dismiss="modal">Deposit</button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- * Send Action Sheet -->



        <div id="app-root">
          <!-- Stats -->

          <!-- * Stats -->

          <!-- Transactions -->

          <div class="section mt-4">
            <div class="section-heading">
              <h2 class="title text-center">Cable TV Subscription</h2>
            </div>
            <div class="section tab-content mt-2 mb-1">
              <div class="row">
                <div class="col-md-12">
                  <div class="row">
                    <div class="col-md-6 mb-3">
                      <div class="card text-center text-white">
                        <div class="card-body"><img src="images/startimes.png" width="80"
                            class="img-fluid img-responsive">
                          <p class="mt-2">
                            StarTimes
                          </p>
                          <p class="mt-2"><a href="buy-startimes" data-toggle="modal" data-target="#dataModal"
                              class="btn btn-primary btn-block buy-airtime">Buy Now</a></p>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-6 mb-3">
                      <div class="card text-center text-white">
                        <div class="card-body"><img src="images/gotv.png" width="80"
                            class="img-fluid img-responsive">
                          <p class="mt-2">
                            GoTV
                          </p>
                          <p class="mt-2"><a href="buy-gotv" data-toggle="modal" data-target="#dataModal"
                              class="btn btn-primary btn-block buy-airtime">Buy Now</a></p>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-6 mb-3">
                      <div class="card text-center text-white">
                        <div class="card-body"><img src="images/dstv.png" width="80"
                            class="img-fluid img-responsive">
                          <p class="mt-2">
                            Dstv
                          </p>
                          <p class="mt-2"><a href="buy-dstv" data-toggle="modal" data-target="#dataModal"
                              class="btn btn-primary btn-block buy-airtime">Buy Now</a></p>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div id="dataModal" tabindex="-1" role="dialog" class="modal fade modalbox">
              <div role="document" class="modal-dialog"><!----></div>
            </div>
          </div>

